{"ast":null,"code":"import HeaderView from './HeaderView.vue';\nimport axios from 'axios';\nexport default {\n  name: 'HomeView',\n\n  data() {\n    return {\n      count: this.count,\n      percent: this.percent\n    };\n  },\n\n  beforeCreate() {\n    document.querySelector('body').setAttribute('style', 'background:black');\n  },\n\n  components: {\n    HeaderView\n  },\n\n  mounted() {\n    let user = localStorage.getItem('user-info');\n\n    if (!user) {\n      this.$router.push({\n        name: 'SignUp'\n      });\n    }\n  },\n\n  async created() {\n    const response = await axios.get(`https://thawing-reaches-79225.herokuapp.com/tutor/count/${localStorage.getItem('userId')}`);\n    this.count = response.data.count;\n    this.percent = (this.count * 100 / 30).toFixed(2);\n  }\n\n};","map":{"version":3,"mappings":"AA2DA,OAAOA,UAAP,MAAuB,kBAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAc;AACVC,MAAI,EAAC,UADK;;AAEVC,MAAI,GAAE;AACF,WAAM;AAEFC,WAAK,EAAC,KAAKA,KAFT;AAGFC,aAAO,EAAC,KAAKA;AAHX,KAAN;AAKH,GARS;;AAUVC,cAAW,GAAK;AACZC,YAAQ,CAACC,aAAT,CAAuB,MAAvB,EAA+BC,YAA/B,CAA4C,OAA5C,EAAqD,kBAArD;AACH,GAZS;;AAaVC,YAAU,EAAC;AACPV;AADO,GAbD;;AAgBVW,SAAO,GAAE;AACL,QAAIC,IAAG,GAAIC,YAAY,CAACC,OAAb,CAAqB,WAArB,CAAX;;AAEA,QAAG,CAACF,IAAJ,EAAS;AACL,WAAKG,OAAL,CAAaC,IAAb,CAAkB;AAACd,YAAI,EAAC;AAAN,OAAlB;AACJ;AAEH,GAvBS;;AAwBV,QAAMe,OAAN,GAAe;AACX,UAAMC,QAAO,GAAI,MAAMjB,KAAK,CAACkB,GAAN,CAAW,2DAA0DN,YAAY,CAACC,OAAb,CAAqB,QAArB,CAA+B,EAApG,CAAvB;AACA,SAAKV,KAAL,GAAac,QAAQ,CAACf,IAAT,CAAcC,KAA3B;AACA,SAAKC,OAAL,GAAe,CAAE,KAAKD,KAAL,GAAW,GAAZ,GAAiB,EAAlB,EAAsBgB,OAAtB,CAA8B,CAA9B,CAAf;AACJ;;AA5BU,CAAd","names":["HeaderView","axios","name","data","count","percent","beforeCreate","document","querySelector","setAttribute","components","mounted","user","localStorage","getItem","$router","push","created","response","get","toFixed"],"sourceRoot":"","sources":["C:\\Users\\Priyank\\OneDrive\\Desktop\\FyndFinalProject\\front-end\\src\\components\\HomeView.vue"],"sourcesContent":["\n<template>\n<