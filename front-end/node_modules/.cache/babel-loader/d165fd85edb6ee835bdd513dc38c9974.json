{"ast":null,"code":"import HeaderView from './HeaderView.vue';\nimport axios from 'axios';\nexport default {\n  name: 'AddCourses',\n  components: {\n    HeaderView\n  },\n\n  data() {\n    return {\n      course: {\n        name: '',\n        description: '',\n        videoId: ''\n      }\n    };\n  },\n\n  beforeCreate() {\n    document.querySelector('body').setAttribute('style', 'background:black');\n  },\n\n  methods: {\n    addCourse() {\n      axios.post(`https://thawing-reaches-79225.herokuapp.com/tutor/add-course/${localStorage.getItem('userId')}`, {\n        //userId:this.course.userId,\n        name: this.course.name,\n        description: this.course.description,\n        videoId: this.course.videoId\n      }).then(result => {\n        alert(\"Course Added Successfully!!\");\n        localStorage.setItem(\"course-info\", JSON.stringify(result.data));\n        this.$router.push({\n          name: 'DisplayCourses'\n        });\n      }).catch(err => {\n        console.log(err);\n      });\n    }\n\n  },\n\n  mounted() {\n    let user = localStorage.getItem('user-info');\n\n    if (!user) {\n      this.$router.push({\n        name: 'SignUp'\n      });\n    }\n  }\n\n};","map":{"version":3,"mappings":"AAqCA,OAAOA,UAAP,MAAuB,kBAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,eAAc;AACVC,MAAI,EAAC,YADK;AAEVC,YAAU,EAAC;AACPH;AADO,GAFD;;AAKVI,MAAI,GAAE;AACF,WAAM;AACFC,YAAM,EAAC;AACHH,YAAI,EAAC,EADF;AAEHI,mBAAW,EAAC,EAFT;AAGHC,eAAO,EAAC;AAHL;AADL,KAAN;AAOH,GAbS;;AAcVC,cAAW,GAAK;AACZC,YAAQ,CAACC,aAAT,CAAuB,MAAvB,EAA+BC,YAA/B,CAA4C,OAA5C,EAAqD,kBAArD;AACH,GAhBS;;AAiBVC,SAAO,EAAC;AACJC,aAAS,GAAE;AACPZ,WAAK,CAACa,IAAN,CAAY,gEAA+DC,YAAY,CAACC,OAAb,CAAqB,QAArB,CAA+B,EAA1G,EAA4G;AACxG;AACAd,YAAI,EAAC,KAAKG,MAAL,CAAYH,IAFuF;AAGxGI,mBAAW,EAAC,KAAKD,MAAL,CAAYC,WAHgF;AAIxGC,eAAO,EAAC,KAAKF,MAAL,CAAYE;AAJoF,OAA5G,EAOCU,IAPD,CAOOC,MAAD,IAAU;AACZC,aAAK,CAAC,6BAAD,CAAL;AACAJ,oBAAY,CAACK,OAAb,CAAqB,aAArB,EAAmCC,IAAI,CAACC,SAAL,CAAeJ,MAAM,CAACd,IAAtB,CAAnC;AACA,aAAKmB,OAAL,CAAaC,IAAb,CAAkB;AAACtB,cAAI,EAAC;AAAN,SAAlB;AACH,OAXD,EAYCuB,KAZD,CAYOC,GAAG,IAAE;AACRC,eAAO,CAACC,GAAR,CAAYF,GAAZ;AACH,OAdD;AAgBJ;;AAlBI,GAjBE;;AAqCVG,SAAO,GAAE;AACL,QAAIC,IAAG,GAAIf,YAAY,CAACC,OAAb,CAAqB,WAArB,CAAX;;AACA,QAAG,CAAEc,IAAL,EAAW;AACP,WAAKP,OAAL,CAAaC,IAAb,CAAkB;AAACtB,YAAI,EAAC;AAAN,OAAlB;AACJ;AACJ;;AA1CU,CAAd","names":["HeaderView","axios","name","components","data","course","description","videoId","beforeCreate","document","querySelector","setAttribute","methods","addCourse","post","localStorage","getItem","then","result","alert","setItem","JSON","stringify","$router","push","catch","err","console","log","mounted","user"],"sourceRoot":"","sources":["C:\\Users\\Priyank\\OneDrive\\Desktop\\FyndFinalProject\\front-end\\src\\components\\AddCourses.vue"],"sourcesContent":["\n<template>\n<fragment>\n    <HeaderView/>\n    <h1 style=\"font-size:3rem;color:white;\"> Welcome to Add Courses Page</h1>\n    <div class=\"container-fluid\">\n        <div class=\"row\">\n            <div class=\"col-lg-4\"></div>\n            <div class=\"col-lg-4\">\n                <form>\n        <div class=\"mt-3\">\n            <label class=\"fs-5 fw-bold text-white text-right\">Enter Name</label>\n            <input  v-model=\"course.name\" type=\"text\" name=\"name\" placeholder=\"Enter Name of the Course\">\n        </div>\n        <div class=\"mt-3\">\n            <label class=\"fs-5 fw-bold text-white text-right\">Enter Description</label>\n            <input v-model=\"course.description\"  type=\"text\" name=\"description\" placeholder=\"Enter Description\" >\n        </div>\n        <div class=\"mt-3\">\n            <label class=\"fs-5 fw-bold text-white text-right\">Enter Video ID</label><br>\n            <label>(https://www.youtube.com/watch?v=<span style=\"color:#069ba2\">V7LwfY5U5WI</span> )</label>\n            <input v-model=\"course.videoId\" type=\"text\" name=\"videoId\" placeholder=\"Enter Video Id\" >\n        </div>\n        <div class=\"mt-4\">\n            <button id=\"addBTN\" v-on:click=\"addCourse\" class=\"btn btn-rounded\">Add the Course +</button>\n        </div>\n    </form>\n            </div>\n            <div class=\"col-lg-4\"></div>\n        </div>\n    </div>\n</fragment>  \n</template>\n\n<script>\n\n\nimport HeaderView from './HeaderView.vue'\nimport axios from 'axios'\nexport default{\n    name:'AddCourses',\n    components:{\n        HeaderView,\n    },\n    data(){\n        return{\n            course:{\n                name:'',\n                description:'',\n                videoId:''\n            }\n        }\n    },\n    beforeCreate () {\n        document.querySelector('body').setAttribute('style', 'background:black')\n    },\n    methods:{  \n        addCourse(){\n            axios.post(`https://thawing-reaches-79225.herokuapp.com/tutor/add-course/${localStorage.getItem('userId')}`,{\n                //userId:this.course.userId,\n                name:this.course.name,\n                description:this.course.description,\n                videoId:this.course.videoId\n                \n            })\n            .then((result)=>{\n                alert(\"Course Added Successfully!!\")\n                localStorage.setItem(\"course-info\",JSON.stringify(result.data))\n                this.$router.push({name:'DisplayCourses'})\n            })\n            .catch(err=>{\n                console.log(err)\n            })\n\n        }\n    },\n    mounted(){\n        let user = localStorage.getItem('user-info');\n        if(!(user)){\n            this.$router.push({name:'SignUp'})\n        }\n    }\n}\n</script>\n\n<style scoped>\ninput{\n    display: block;\n    width:100%;\n    padding:1%;\n}\nlabel{\n    float:left;\n}\n#addBTN{\n    background:#774c9e;\n    display:block;\n    width:100%;\n    padding:2%;\n    color: black;\n    font-weight: 800;\n}\n</style>"]},"metadata":{},"sourceType":"module"}