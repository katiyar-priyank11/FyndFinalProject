{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'LoginIn',\n\n  data() {\n    return {\n      email: '',\n      password: ''\n    };\n  },\n\n  beforeCreate() {\n    document.querySelector('body').setAttribute('style', 'background:black');\n  },\n\n  methods: {\n    async login() {\n      let result = await axios.post(\"https://thawing-reaches-79225.herokuapp.com/tutor/login\", {\n        email: this.email,\n        password: this.password\n      });\n\n      if (result.status == 200) {\n        console.log(\"Hello from vue login\");\n        console.log(result.data.user);\n        localStorage.setItem(\"user-info\", JSON.stringify(result.data));\n        localStorage.setItem(\"userId\", result.data.user[0]._id);\n        this.$router.push('/home');\n      } else {\n        alert(\"Password is incorrect!!\");\n      }\n    }\n\n  },\n\n  mounted() {\n    let user = localStorage.getItem('user-info');\n\n    if (user) {\n      this.$router.push({\n        name: 'HomeView'\n      });\n    }\n  }\n\n};","map":{"version":3,"mappings":"AA+BA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;AACXC,MAAI,EAAC,SADM;;AAEXC,MAAI,GAAE;AACF,WAAM;AACFC,WAAK,EAAC,EADJ;AAEFC,cAAQ,EAAC;AAFP,KAAN;AAIH,GAPU;;AAQVC,cAAW,GAAK;AACbC,YAAQ,CAACC,aAAT,CAAuB,MAAvB,EAA+BC,YAA/B,CAA4C,OAA5C,EAAqD,kBAArD;AACH,GAVU;;AAWXC,SAAO,EAAC;AACJ,UAAMC,KAAN,GAAa;AACT,UAAIC,MAAK,GAAI,MAAMX,KAAK,CAACY,IAAN,CAAW,yDAAX,EAAqE;AACpFT,aAAK,EAAC,KAAKA,KADyE;AAEpFC,gBAAQ,EAAC,KAAKA;AAFsE,OAArE,CAAnB;;AAIA,UAAGO,MAAM,CAACE,MAAP,IAAe,GAAlB,EAAsB;AAClBC,eAAO,CAACC,GAAR,CAAY,sBAAZ;AACAD,eAAO,CAACC,GAAR,CAAYJ,MAAM,CAACT,IAAP,CAAYc,IAAxB;AACAC,oBAAY,CAACC,OAAb,CAAqB,WAArB,EAAiCC,IAAI,CAACC,SAAL,CAAeT,MAAM,CAACT,IAAtB,CAAjC;AACAe,oBAAY,CAACC,OAAb,CAAqB,QAArB,EAA8BP,MAAM,CAACT,IAAP,CAAYc,IAAZ,CAAiB,CAAjB,EAAoBK,GAAlD;AACA,aAAKC,OAAL,CAAaC,IAAb,CAAkB,OAAlB;AACJ,OANA,MAOI;AACAC,aAAK,CAAC,yBAAD,CAAL;AACJ;AAEJ;;AAjBI,GAXG;;AA8BXC,SAAO,GAAE;AACL,QAAIT,IAAG,GAAIC,YAAY,CAACS,OAAb,CAAqB,WAArB,CAAX;;AACA,QAAGV,IAAH,EAAQ;AACJ,WAAKM,OAAL,CAAaC,IAAb,CAAkB;AAACtB,YAAI,EAAC;AAAN,OAAlB;AACJ;AACJ;;AAnCW,CAAf","names":["axios","name","data","email","password","beforeCreate","document","querySelector","setAttribute","methods","login","result","post","status","console","log","user","localStorage","setItem","JSON","stringify","_id","$router","push","alert","mounted","getItem"],"sourceRoot":"","sources":["C:\\Users\\Priyank\\OneDrive\\Desktop\\FyndFinalProject\\front-end\\src\\components\\LogIn.vue"],"sourcesContent":["<template>\n<fragment>\n    <img id=\"loginLogo\" src=\"../assets/fyndLogo.png\" alt=\"\" srcset=\"\">\n    <h1 id=\"loginheading\" class=\"fw-bold text-white\">Log In</h1>\n    <div class=\"container-fluid\">\n        <div class=\"row\">\n            <div class=\"col-lg-4\"></div>\n            <div class=\"col-lg-4\">\n                <div class=\"mt-4\">\n                    <label class=\"fs-5 fw-bold text-white\">Email</label>\n                    <input type=\"text\" v-model=\"email\" placeholder=\"Enter Email\">\n                </div>\n                <div class=\"mt-4\">\n                    <label class=\"fs-5 fw-bold text-white\">Password</label>\n                    <input type=\"password\" v-model=\"password\" placeholder=\"Enter Password\">\n                </div>\n                <div class=\"mt-4\">\n                    <button id=\"loginBTN\" v-on:click=\"login\" class=\"btn btn-rounded\">Log In</button>\n                </div>\n                <div class=\"mt-4\">\n                    <router-link id=\"signUpId\" to=\"/sign-up\" style=\"color:green\">Create an account? Sign Up</router-link>\n                </div>    \n            </div>\n            <div class=\"col-lg-4\"></div>\n        </div>\n        \n    </div>\n</fragment>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n    name:'LoginIn',\n    data(){\n        return{\n            email:'',\n            password:''\n        }\n    },\n     beforeCreate () {\n        document.querySelector('body').setAttribute('style', 'background:black')\n    },\n    methods:{\n        async login(){\n            let result = await axios.post(\"https://thawing-reaches-79225.herokuapp.com/tutor/login\",{\n                email:this.email,\n                password:this.password\n            })\n            if(result.status==200){\n                console.log(\"Hello from vue login\")\n                console.log(result.data.user)\n                localStorage.setItem(\"user-info\",JSON.stringify(result.data))\n                localStorage.setItem(\"userId\",result.data.user[0]._id)\n                this.$router.push('/home')\n            }\n            else{\n                alert(\"Password is incorrect!!\")\n            }\n            \n        }\n    },\n    mounted(){\n        let user = localStorage.getItem('user-info');\n        if(user){\n            this.$router.push({name:'HomeView'})\n        }\n    }\n}\n</script>\n\n<style scoped>\n#loginLogo{\n    width: 10%;\n    margin-top: 5%;\n}\n#loginheading{\n    font-size: 3rem;\n}\n\ninput{\n    display: block;\n    width:100%;\n    padding:1%;\n}\nlabel{\n    float: left;\n}\n#loginBTN{\n    background:#774c9e;\n    display:block;\n    width:100%;\n    padding:2%;\n    color: black;\n    font-weight: 800;\n}\n#signUpId{\n    color: green;\n    font-weight: 600;\n    text-decoration: none;\n}\n</style>"]},"metadata":{},"sourceType":"module"}