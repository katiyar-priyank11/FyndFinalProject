{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"SignUp\",\n\n  data() {\n    return {\n      name: \"\",\n      email: \"\",\n      password: \"\"\n    };\n  },\n\n  beforeCreate() {\n    document.querySelector(\"body\").setAttribute(\"style\", \"background:black\");\n  },\n\n  methods: {\n    async signUp() {\n      let result = await axios.post(\"https://thawing-reaches-79225.herokuapp.com/tutor/user\", {\n        name: this.name,\n        email: this.email,\n        password: this.password\n      });\n\n      if (result.status == 201) {\n        console.log(\"HELLO FROM VUE SIGN UP\");\n        this.$router.push(\"/login\");\n        console.log(result.data.user);\n        localStorage.setItem(\"user-info\", JSON.stringify(result.data));\n        localStorage.setItem(\"userId\", result.data.user._id);\n      } else if (result.status == 409) {\n        console.log(\"Email ID already registered!!\");\n        alert(\"Email Id already registered!!\");\n      } else {\n        alert(\"Some error occurred\");\n      }\n    }\n\n  },\n\n  mounted() {\n    let user = localStorage.getItem(\"user-info\");\n\n    if (user) {\n      this.$router.push(\"/login\");\n    }\n  }\n\n};","map":{"version":3,"mappings":"AA2CA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;AACbC,MAAI,EAAE,QADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLD,UAAI,EAAE,EADD;AAELE,WAAK,EAAE,EAFF;AAGLC,cAAQ,EAAE;AAHL,KAAP;AAKD,GARY;;AASbC,cAAY,GAAG;AACbC,YAAQ,CAACC,aAAT,CAAuB,MAAvB,EAA+BC,YAA/B,CAA4C,OAA5C,EAAqD,kBAArD;AACD,GAXY;;AAabC,SAAO,EAAE;AACP,UAAMC,MAAN,GAAe;AACb,UAAIC,MAAK,GAAI,MAAMX,KAAK,CAACY,IAAN,CACjB,wDADiB,EAEjB;AACEX,YAAI,EAAE,KAAKA,IADb;AAEEE,aAAK,EAAE,KAAKA,KAFd;AAGEC,gBAAQ,EAAE,KAAKA;AAHjB,OAFiB,CAAnB;;AAQA,UAAIO,MAAM,CAACE,MAAP,IAAiB,GAArB,EAA0B;AACxBC,eAAO,CAACC,GAAR,CAAY,wBAAZ;AACA,aAAKC,OAAL,CAAaC,IAAb,CAAkB,QAAlB;AACAH,eAAO,CAACC,GAAR,CAAYJ,MAAM,CAACT,IAAP,CAAYgB,IAAxB;AACAC,oBAAY,CAACC,OAAb,CAAqB,WAArB,EAAkCC,IAAI,CAACC,SAAL,CAAeX,MAAM,CAACT,IAAtB,CAAlC;AAEAiB,oBAAY,CAACC,OAAb,CAAqB,QAArB,EAA+BT,MAAM,CAACT,IAAP,CAAYgB,IAAZ,CAAiBK,GAAhD;AACF,OAPA,MAQK,IAAGZ,MAAM,CAACE,MAAP,IAAiB,GAApB,EAAwB;AAC3BC,eAAO,CAACC,GAAR,CAAY,+BAAZ;AACAS,aAAK,CAAC,+BAAD,CAAL;AACF,OAHK,MAID;AAEFA,aAAK,CAAC,qBAAD,CAAL;AACF;AACD;;AA1BM,GAbI;;AAyCbC,SAAO,GAAG;AACR,QAAIP,IAAG,GAAIC,YAAY,CAACO,OAAb,CAAqB,WAArB,CAAX;;AACA,QAAIR,IAAJ,EAAU;AACR,WAAKF,OAAL,CAAaC,IAAb,CAAkB,QAAlB;AACF;AACD;;AA9CY,CAAf","names":["axios","name","data","email","password","beforeCreate","document","querySelector","setAttribute","methods","signUp","result","post","status","console","log","$router","push","user","localStorage","setItem","JSON","stringify","_id","alert","mounted","getItem"],"sourceRoot":"","sources":["C:\\Users\\Priyank\\OneDrive\\Desktop\\FyndFinalProject\\front-end\\src\\components\\SignUp.vue"],"sourcesContent":["<template>\n<fragment>\n  <img id=\"logo\" src=\"../assets/fyndLogo.png\" alt=\"\" srcset=\"\" />\n  <h1 id=\"registerheading\" class=\"fw-bold text-white\">Register</h1>\n  <div class=\"container-fluid\">\n    <div class=\"row\">\n      <div class=\"col-lg-4\"></div>\n      <div class=\"col-lg-4\">\n        <div class=\"mt-4\">\n          <label class=\"fs-5 fw-bold text-white\">Name</label>\n          <input\n            class=\"border-rounded\"\n            type=\"text\"\n            v-model=\"name\"\n            placeholder=\"Enter Name\"\n          />\n        </div>\n        <div class=\"mt-4\">\n          <label class=\"fs-5 fw-bold text-white\">Email</label>\n          <input type=\"text\" v-model=\"email\" placeholder=\"Enter Email\" />\n        </div>\n        <div class=\"mt-4\">\n          <label class=\"fs-5 fw-bold text-white\">Password</label>\n          <input type=\"password\" v-model=\"password\" placeholder=\"Enter Password\" />\n        </div>\n        <div class=\"mt-4\">\n          <button id=\"registerBTN\" v-on:click=\"signUp\" class=\"btn btn-rounded\">\n            Sign Up\n          </button>\n        </div>\n        <div class=\"mt-4\">\n          <router-link to=\"/login\" id=\"loginID\"\n            >Already have an account? Log In</router-link\n          >\n        </div>\n      </div>\n      <div class=\"col-lg-4\"></div>\n    </div>\n  </div>\n</fragment>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"SignUp\",\n  data() {\n    return {\n      name: \"\",\n      email: \"\",\n      password: \"\",\n    };\n  },\n  beforeCreate() {\n    document.querySelector(\"body\").setAttribute(\"style\", \"background:black\");\n  },\n\n  methods: {\n    async signUp() {\n      let result = await axios.post(\n        \"https://thawing-reaches-79225.herokuapp.com/tutor/user\",\n        {\n          name: this.name,\n          email: this.email,\n          password: this.password,\n        }\n      );\n      if (result.status == 201) {\n        console.log(\"HELLO FROM VUE SIGN UP\");\n        this.$router.push(\"/login\");\n        console.log(result.data.user);\n        localStorage.setItem(\"user-info\", JSON.stringify(result.data));\n\n        localStorage.setItem(\"userId\", result.data.user._id);\n      }\n      else if(result.status == 409){\n        console.log(\"Email ID already registered!!\")\n        alert(\"Email Id already registered!!\")\n      }\n      else{\n\n        alert(\"Some error occurred\")\n      }\n    },\n  },\n  mounted() {\n    let user = localStorage.getItem(\"user-info\");\n    if (user) {\n      this.$router.push(\"/login\");\n    }\n  },\n};\n</script>\n\n<style scoped>\n#logo {\n  width: 10%;\n}\n#registerheading {\n  font-size: 3rem;\n}\ninput {\n  display: block;\n  width: 100%;\n  padding: 1%;\n}\nlabel {\n  float: left;\n}\n#registerBTN {\n  background: #774c9e;\n  display: block;\n  width: 100%;\n  padding: 2%;\n  color: black;\n  font-weight: 800;\n}\n#loginID {\n  color: green;\n  font-weight: 600;\n  text-decoration: none;\n}\n</style>"]},"metadata":{},"sourceType":"module"}